$TTL    604800
@       IN      SOA     ocp-svc.ocp.nsth.demo. contact.ocp.nsth.demo (
                  1     ; Serial
             604800     ; Refresh
              86400     ; Retry
            24192     ; Expire
             604800     ; Minimum
)
