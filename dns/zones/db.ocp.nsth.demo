$TTL    604800
@       IN      SOA     ocp-svc.ocp.nsth.demo. contact.ocp.nsth.demo (
                  1     ; Serial
             604800     ; Refresh
              86400     ; Retry
            24192     ; Expire
             604800     ; Minimum
)
        IN      NS      ocp-svc

ocp-svc.ocp.nsth.demo.          IN      A       192.168.44.1

; Temp Bootstrap Node
ocp-bootstrap.lab.ocp.nsth.demo.        IN      A      192.168.44.2

; Control Pnsth.demoe Nodes
ocp-cp-1.lab.ocp.nsth.demo.         IN      A      192.168.44.11
ocp-cp-2.lab.ocp.nsth.demo.         IN      A      192.168.44.12
ocp-cp-3.lab.ocp.nsth.demo.         IN      A      192.168.44.13

; Worker Nodes
ocp-w-1.lab.ocp.nsth.demo.        IN      A      192.168.44.21
ocp-w-2.lab.ocp.nsth.demo.        IN      A      192.168.44.22

; OpenShift Internal - Load balancer
api.lab.ocp.nsth.demo.        IN    A    192.168.44.1
api-int.lab.ocp.nsth.demo.    IN    A    192.168.44.1
*.apps.lab.ocp.nsth.demo.     IN    A    192.168.44.1

; ETCD Cluster
etcd-0.lab.ocp.nsth.demo.    IN    A     192.168.44.11
etcd-1.lab.ocp.nsth.demo.    IN    A     192.168.44.12
etcd-2.lab.ocp.nsth.demo.    IN    A     192.168.44.13

; OpenShift Internal SRV records (cluster name = lab)
_etcd-server-ssl._tcp.lab.ocp.nsth.demo.    86400     IN    SRV     0    10    2380    etcd-0.lab
_etcd-server-ssl._tcp.lab.ocp.nsth.demo.    86400     IN    SRV     0    10    2380    etcd-1.lab
_etcd-server-ssl._tcp.lab.ocp.nsth.demo.    86400     IN    SRV     0    10    2380    etcd-2.lab

oauth-openshift.apps.lab.ocp.nsth.demo.     IN     A     192.168.44.1
console-openshift-console.apps.lab.ocp.nsth.demo.     IN     A     192.168.44.1
